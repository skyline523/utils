{
  "name": "@xperia11/utils",
  "type": "module",
  "version": "0.0.9",
  "packageManager": "pnpm@9.1.1",
  "description": "This is my commonly used tool chain",
  "author": "Leet <programingwithli@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/skyline523/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skyline523/utils.git"
  },
  "bugs": "https://github.com/skyline523/utils/issues",
  "keywords": [
    "utils",
    "config",
    "unocss"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "commit": "git-cz",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.1",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.8",
    "@types/node": "^20.12.12",
    "bumpp": "^9.4.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.3.0",
    "esno": "^4.7.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "pnpm": "^9.1.1",
    "rimraf": "^5.0.7",
    "standard-version": "^9.5.0",
    "tsup": "^8.2.3",
    "typescript": "^5.4.5",
    "unocss": "^0.61.0",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
